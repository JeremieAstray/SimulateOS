<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.os.controller.MainController">
    <children>
        <Button fx:id="initDiskInfoButton" layoutX="14.0" layoutY="41.0" onAction="#initDiskInfo" prefHeight="58.0" prefWidth="123.0" text="初始化磁盘信息" />
        <Button fx:id="loadFatTableButton" layoutX="155.0" layoutY="41.0" onAction="#loadFatTable" prefHeight="58.0" prefWidth="123.0" text="加载Fat表" />
      <TableView fx:id="fatTable" layoutX="622.0" layoutY="35.0" prefHeight="536.0" prefWidth="152.0">
        <columns>
          <TableColumn fx:id="diskNumber1" prefWidth="72.0" text="盘块号" />
          <TableColumn fx:id="diskNumber2" prefWidth="78.0" text="指向的盘块号" />
        </columns>
      </TableView>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="622.0" layoutY="13.0" prefHeight="21.0" prefWidth="152.0" text="Fat表" textAlignment="CENTER">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
